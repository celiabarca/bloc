{# empty Twig template #}

{% extends 'base.html.twig' %}
{% form_theme form _self %}



{% block body %}
    <head>
    {# ... #}
 
    {% block stylesheets %}
        <link href="{{ asset('css/mycss.css') }}" type="text/css" rel="stylesheet" />
    {% endblock %}
    </head>
    <body>
        
    <div class="contentform registro">
        <h2>Sign up, please</h2>
        {{  form_start(form)}}
        {{  form_widget(form) }}
        {{  form_end(form) }}           
        {{  form_errors(form)}}        
    </div>
       </body>
{% endblock %}

{% block form_errors %}
{% if errors|length >0 %}
    <ul>
        {% for error in errors %}
        <li class="error-message">{{error.message}}</li>
        {% endfor %}
        </ul>
{%endif%}    
{% endblock form_errors %}

